{
  "extensions": {
    "txt,c,cpp,js,conf,md,sh,json,map,yml,xml,py,log,cs,ini,csv": "leafpad $arg",
    "avi,mkv,flac,mp3,wav,mp4,mov": "/home/igor/arch/scripts/vlc.sh $arg",
    "jpg,jpeg,svg,png,bmp,gif,tga": "gpicview $arg",
    "gz,zip,rar,zst": "xarchiver $arg",
    "torrent": "qbittorrent $arg",
    "pdf": "mupdf -r 96 $arg",
    "html": "chromium $arg"
  },
  "mimetypes": {
    "text/*": "leafpad $arg",
    "video/*": "/home/igor/arch/scripts/vlc.sh $arg",
    "media/video": "/home/igor/arch/scripts/vlc.sh $arg",
    "document/pdf": "mupdf -r 96 $arg",
    "inode/directory": "qtfm $arg",
    "inode/x-empty": "leafpad $arg",
    "application/octet-stream": "leafpad $arg"
  },
  "protocols": {
    "http://*": "chromium $arg",
    "https://*": "chromium $arg",
    "slack://*": "slack $arg",
    "file://*": "qtfm $arg"
  }
}

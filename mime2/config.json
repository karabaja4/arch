{
  "extensions": {
    "txt": "featherpad $arg",
    "avi": "vlc $arg",
    "pdf": "mupdf -r 96 $arg"
  },
  "mimetypes": {
    "text/*": "featherpad $arg",
    "video/*": "vlc $arg",
    "media/video": "vlc $arg",
    "document/pdf": "mupdf -r 96 $arg",
    "inode/directory": "qtfm $pwd"
  },
  "protocols": {
    "http://*": "chromium $arg",
    "https://*": "chromium $arg",
    "slack://*": "slack $arg"
  }
}
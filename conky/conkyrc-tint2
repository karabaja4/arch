	"net": {
		"down": "${downspeedf enp2s0}",
		"up": "${upspeedf enp2s0}"
	},
	"cpu": {
		"perc": "${cpu}",
		"freq": "${freq}",
		"temp": "${hwmon 5 temp 1}"
	},
	"mem": {
		"perc": "${memperc}",
		"used": "${mem}",
		"max": "${memmax}"
	}
}
]];

{
  "extensions": {
    "txt,c,cpp,js,conf,md,sh,json,map,yml,xml,py,log,cs,ini,csv,cfg": "qtextpad $arg",
    "avi,mkv,flac,mp3,wav,mp4,mov,ogg": "vlc $arg",
    "jpg,jpeg,svg,png,bmp,gif,tga": "feh --no-menus --scale-down --geometry 1280x720 --image-bg black --start-at $arg",
    "gz,zip,rar,zst,tar,7z,iso": "xarchiver $arg",
    "torrent": "qbittorrent $arg",
    "pdf": "mupdf -r 96 $arg",
    "html": "chromium $arg",
    "exe": "wine $arg"
  },
  "mimetypes": {
    "text/*": "qtextpad $arg",
    "video/*": "vlc $arg",
    "image/*": "feh --no-menus --scale-down --geometry 1280x720 --image-bg black --start-at $arg",
    "media/video": "vlc $arg",
    "document/pdf": "mupdf -r 96 $arg",
    "inode/directory": "qtfm $arg",
    "inode/x-empty": "qtextpad $arg",
    "application/octet-stream": "qtextpad $arg"
  },
  "protocols": {
    "http://": "firefox-socket-control $arg",
    "https://": "firefox-socket-control $arg",
    "slack://": "slack $arg",
    "file://": "qtfm $arg",
    "vscode://": "code $arg"
  }
}
{
  "extensions": {
    "txt,c,cpp,js,conf,md,sh,json,map,yml,xml,py,log,cs,ini,csv,cfg": "qtextpad $arg",
    "avi,mkv,flac,mp3,wav,mp4,mov,ogg,aac,opus": "vlc $arg",
    "jpg,jpeg,svg,png,bmp,gif,tga": "/home/igor/arch/feh/feh.sh $arg",
    "gz,zip,rar,zst,tar,7z,iso,bz2,xz": "xarchiver $arg",
    "torrent": "/home/igor/arch/scripts/torrent.sh $arg",
    "pdf": "mupdf -r 138 $arg",
    "htm,html": "chromium $arg",
    "exe": "wine $arg",
    "doc,xls,ppt,docx,xlsx,pptx,odt": "onlyoffice-desktopeditors $arg"
  },
  "mimetypes": {
    "text/*": "qtextpad $arg",
    "video/*": "vlc $arg",
    "image/*": "/home/igor/arch/feh/feh.sh $arg",
    "media/video": "vlc $arg",
    "document/pdf": "mupdf -r 138 $arg",
    "inode/directory": "/home/igor/arch/lf/lf.sh $arg",
    "inode/x-empty": "qtextpad $arg",
    "application/octet-stream": "qtextpad $arg"
  },
  "protocols": {
    "http://": "chromium $arg",
    "https://": "chromium $arg",
    "slack://": "slack $arg",
    "file://": "/home/igor/arch/lf/lf.sh $arg",
    "vscode://": "code $arg",
    "msteams:/l/": "/home/igor/arch/teams/teams.sh $arg",
    "zoommtg://": "zoom $arg"
  }
}
conky.config = {
  out_to_x = false,
  out_to_console = true,
  update_interval = 1,
  short_units	= true
};

conky.text = [[
{
  "net": {
    "down": "${downspeedf enp56s0u1u2c2}",
    "up": "${upspeedf enp56s0u1u2c2}"
  },
  "cpu": {
    "perc": "${cpu}",
    "freq": "${freq}",
    "temp": "${hwmon 7 temp 1}"
  },
  "bat": {
    "perc": "${battery_percent}",
    "time": "${battery_time}"
  },
  "mem": {
    "perc": "${memperc}",
    "used": "${mem}",
    "max": "${memmax}"
  }
}
]];
